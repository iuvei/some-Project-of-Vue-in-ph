        // 获取下线佣金列表
        proxy_commissions() {
            this.user = JSON.parse(sessionStorage.getItem('user'));
            requestOpt.reqGet(
                "proxy_commissions?proxy_type=" +
                this.proxy_type +
                "&date_from=" +
                this.dai_li_start_value +
                "&date_to=" +
                this.dai_li_end_value,
                this.user.token,
                res => {
                // this.loading = false;
                if (res.data.status === 1) {
                   this.yong_jin_data_list  = res.data.data.list;
                    if (res.data.data.list.downline_list.length <= 0) {
                         this.is_show_yong_jin_nodata = true;
                    } else {
                         this.is_show_yong_jin_nodata = false;
                    }
                } 
                else if(res.data.status == 0 && res.data.code == -2){
                    el_tip(res.data.msg + ' 请重新登录');
                    sessionStorage.removeItem('user')
                    
                    setTimeout(()=>{
                        window.location.reload();
                        this.$root.bus.$emit("sendData",true);
                    },1000)
                }
                else {
                    el_tip(res.data.msg);
                }
                },
                err => {
                     if(err.data.mag){
                            el_tip(err.data.msg);
                        }
                }
            );
        },
    // 报表统计
        is_show_bao_biao_cont_btn(index){
            this.is_show_bao_biao_cont = [];
            this.is_show_bao_biao_cont[index] = true;
            this.bao_biao_text_title = index == 0 ? '个人报表' : '团队报表'; 
            this.bao_biao_type = index == 0 ? 1 : 2;
            this.get_bao_biao_data();
        },
        select_bb_list(val,name){
            this.bao_biao_tab_text_title = name;
            this.bao_biao_start_value = val;
            this.get_bao_biao_data();
        },
        // select_bb1_list(val,name){
        //     this.bao_biao_tab_text_title = name;
        //     this.bao_biao_start_value = val;
        //     this.get_bao_biao_data();
        // },
        // select_bb2_list(val,name){
        //     this.bao_biao_tab_text_title = name;
        //     this.bao_biao_start_value = val;
        //     this.get_bao_biao_data();
        // },
        second_change() {
            this.bao_biao_start_value = dtFormate.date(this.bao_biao_start_value);
            this.bao_biao_end_value = dtFormate.date(this.bao_biao_end_value);
            for(var obj in this.dtbtnList){
                if(obj['val'] !== this.bao_biao_start_value){
                   this.bao_biao_tab_text_title = '自定义';
                }
            }
            this.get_bao_biao_data();
        },
        // 获取报表统计数据

        get_bao_biao_data(){
            this.is_show_get_bao_biao_data = false;
             this.user = JSON.parse(sessionStorage.getItem('user'));
             requestOpt.reqGet(
                "proxy_stat?type=" +
                this.bao_biao_type +
                "&date_from=" +
                this.bao_biao_start_value +
                "&date_to=" +
                this.bao_biao_end_value,
                this.user.token,
                res => {
                if (res.data.status === 1) {
                    this.bao_biao_data = res.data.data.statistic;
                    this.is_show_get_bao_biao_data = true;
                    // console.log(this.bao_biao_data);
                } 
                else if(res.data.status == 0 && res.data.code == -2){
                    el_tip(res.data.msg + ' 请重新登录');
                    sessionStorage.removeItem('user')
                    
                    setTimeout(()=>{
                        window.location.reload();
                        this.$root.bus.$emit("sendData",true);
                    },1000)
                }
                else {
                        el_tip(res.data.msg);
                    }
                },
                err => {
                    if(err){
                        el_tip(err.data.msg);
                    }
                }
            );
        }


    
    },
    mounted(){
        this.get_open_info();
    },
    filters:{
        transImg(pic){
            let default_img = require('../../image/default_user_ava.png');
            pic = pic == 'default' ? default_img : pic;
            return pic;
        },

        transWinningStatus(val){
             switch(val) {
                case 0 :
                return '待开奖';
                break;
                case 1 :
                return '已中奖';
                break;
                case 2 :
                return '未中奖';
                break;
                case 3 :
                return '开奖中';
                break;
                case 4 :
                return '撤销';
                break;
                case 5 :
                return '作废';
                break;
                default:
                break;
            }
        },
        transWinningDate(val){
            switch(val){
                case 'today' :
                    return '今天';
                    break;
                case 'yesterday' :
                    return '昨天';
                    break;
                case 'week' :
                    return '近7天';
                    break;
                case 'mouth' :
                    return '近30天';
                    break;
                case 'total' :
                    return '历史总计';
                    break;
            }

        },
        transWinningIndex(str){
            return str.split(",");
        }
        
    }

}
    
</script>


