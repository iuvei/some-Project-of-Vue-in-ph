        height 6.57rem
        background #fff
        border-radius 0.08rem
        h4 {
            font-size .36rem
            width 100%
            padding .2rem
            border-bottom 1px solid #ccc
            text-align center
        }
        input {
            -webkit-appearance none
            border none
            background transparent
        }
        .bets-setion {
            padding .15rem 0.2rem
            .bets-rotats {
            display flex
            justify-content space-between
            display -webkit-flex
            -webkit-justify-content space-between
            margin-bottom .2rem
            }

            .bets-input {
              display flex
              align-items center
              margin-top .2rem
              input {
                width 2rem
                padding .1rem 0
                border 1px solid #ccc
                text-align center
              }
              span {
                padding  .16rem
                margin-left .11rem
                background #fff
                border 1px solid #ddd
                color #000
                &:first-child {
                  flex-basis 1.8rem
                  border 0
                  color #000000
                  background #fff
                  padding 0
                  margin-left 0
                }
                &.cashActive {
                  background #e8601d
                  color #fff
                  border-color transparent
                }
                  /*&:last-child {
                  padding .1rem .2rem
                  background #e8601d
                  color #fff
                  }*/
              }
            }
            .bets-fetrue {
            width 100%
            display flex
            display -webkit-flex
            margin-top 0.2rem
            .bets-fetrue-item {
                display flex
                justify-content space-between
                align-items center
                display -webkit-flex
                -webkit-justify-content space-between
                -webkit-align-items center
                width 50%
                margin-right 0.3rem
                .b-feture {
                display flex
                justify-content space-between
                align-items center
                display -webkit-flex
                -webkit-justify-content space-between
                -webkit-align-items center
                border 1px solid #ccc
                border-radius 0.08rem
                }
                input[type=button] {
                border 0px solid #ccc
                padding .18rem .2rem
                border-radius 0
                &:first-child {
                    border-right 1px solid #ccc
                }
                &:last-child {
                    border-left 1px solid #ccc
                }
                }
                input[type=text] {
                width .88rem
                padding .11rem
                text-align center
                }
            }
            }
            .bets-count {
            padding-top .2rem
            }
            .bets-winning {
            padding-top .2rem
            .sum {
                display: inline-block;
                min-width 2.7rem

                text-align: center
                color #35b5fe
            }
            .fr {
                float right
            }
            }
        }
        .bets-foot {
            position absolute
            bottom 0
            display flex
            display -webkit-flex
            width 100%
            border-top 1px solid #ccc
            input {
            width 50%
            padding .2rem
            &:last-child {
              color #fff
              background #ff3a3b
            }
            }
        }
        }
    }

    .shaked {
        transition all 300ms
        transform rotate(0deg)
        animation ani_shake 300ms
    }

    @keyframes ani_shake {
        0% {
        transform rotate(0deg)
        }
        25% {
        transform rotate(30deg)
        }
        50% {
        transform rotate(0deg)
        }
        75% {
        transform rotate(-90deg)
        }
        100% {
        transform rotate(0deg)
        }
    }

